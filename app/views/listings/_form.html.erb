<%= form_for @listing do |f| %>
  <%= f.label :role %>
  <%= f.text_field :role %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :company %>
  <%= f.text_field :company %>

  <%= f.label :company_information %>
  <%= f.text_area :company_information %>

  <%= f.submit :Submit %>
<% end %>